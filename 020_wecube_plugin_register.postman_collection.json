		{
			"name": "get_install_package_id",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "9e68446e-9e32-48df-885f-ded93710f6fb",
						"exec": [
							"pm.test(\"get package id\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.status).to.eql(\"OK\");",
							"    var curPackageId = jsonData.data.packageId",
							"    pm.environment.set(\"cur_package_id\", curPackageId);",
							"    if (!curPackageId) {",
							"        var tmpInterval = setInterval(function(){",
							"            console.info(\"wait 5s\")",
							"            pm.sendRequest({",
							"                url: \"http://\"+ pm.environment.get(\"domain\") + \"/platform/v1/plugin-artifacts/pull-requests/\" + pm.environment.get(\"cur_package_request_id\"),",
							"                method: 'GET',",
							"                header: {",
							"                    'Accept': 'application/json',",
							"                    'Content-Type': 'application/json;charset=UTF-8',",
							"                    'authorization': pm.environment.get(\"authorization\"),",
							"                }",
							"            }, function (err, res) {",
							"                if (res.json().data.packageId) {",
							"                    curPackageId = res.json().data.packageId",
							"                }           ",
							"            });",
							"            if (curPackageId) {",
							"                pm.environment.set(\"cur_package_id\", curPackageId);",
							"                clearInterval(tmpInterval);",
							"            }",
							"        }, 5000);",
							"    }",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Accept",
						"value": "application/json, text/plain, */*"
					},
					{
						"key": "Authorization",
						"value": "{{authorization}}"
					},
					{
						"key": "User-Agent",
						"value": "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/72.0.3626.119 Safari/537.36"
					}
				],
				"url": {
					"raw": "http://{{domain}}/platform/v1/plugin-artifacts/pull-requests/{{cur_package_request_id}}",
					"protocol": "http",
					"host": [
						"{{domain}}"
					],
					"path": [
						"platform",
						"v1",
						"plugin-artifacts",
						"pull-requests",
						"{{cur_package_request_id}}"
					]
				}
			},
			"response": []
		},
